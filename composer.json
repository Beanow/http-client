{
    "name": "react/http-client",
    "description": "Asynchronous HTTP client library.",
    "keywords": ["http"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "guzzle/parser": "~3.0",
        "react/socket-client": "0.4.*",
        "react/dns": "0.4.*"
    },
    "autoload": {
        "psr-4": {
            "React\\HttpClient\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    }
}
